{
  "name": "tracer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "run-s test:*",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-08-05 --proxy 5173 -- npm run dev",
    "pages:deploy": "NODE_VERSION=18 npm run build && wrangler pages deploy .svelte-kit/cloudflare --project-name tracer",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.3",
    "@sveltejs/kit": "^1.25.1",
    "@typescript-eslint/eslint-plugin": "^6.7",
    "@typescript-eslint/parser": "^6.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0",
    "prettier-plugin-svelte": "^3.0",
    "svelte": "^4.2",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "wrangler": "^3.10.1"
  },
  "type": "module"
}
